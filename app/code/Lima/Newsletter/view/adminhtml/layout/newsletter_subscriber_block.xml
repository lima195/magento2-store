<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
	<body>
		<referenceBlock name="adminhtml.newslettrer.subscriber.grid.columnSet">
			<block class="Magento\Backend\Block\Widget\Grid\Column" as="mobile_number">
				<arguments>
					<argument name="header" xsi:type="string" translate="true">Mobile Number</argument>
					<argument name="index" xsi:type="string">mobile_number</argument>
					<argument name="header_css_class" xsi:type="string">col-mobilenumber</argument>
					<argument name="column_css_class" xsi:type="string">ccol-mobilenumber</argument>
				</arguments>
			</block>
		</referenceBlock>
	</body>
</page>